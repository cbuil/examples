{
  "name": "example-configure-sparql-http-solid",
  "version": "1.0.0",
  "description": "An example illustrating how to replace the HTTP actor with a Solid HTTP actor",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "bin": {
    "my-comunica": "./bin/query.js",
    "my-comunica-http": "./bin/http.js",
    "my-comunica-dynamic": "./bin/query-dynamic.js"
  },
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/example-configure-sparql-http-solid",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/example-configure-sparql-http-solid/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/example-configure-sparql-http-solid/^1.0.0/components/": "components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/example-configure-sparql-http-solid/^1.0.0/config/": "config/"
  },
  "files": [
    "components",
    "config",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "index.js",
    "index.d.ts",
    "index-browser.d.ts",
    "index-browser.js",
    "engine-default.js"
  ],
  "dependencies": {
    "@comunica/actor-init-sparql": "^1.11.0",
    "@comunica/actor-http-solid-auth-fetch": "^1.0.1",
    "@types/minimist": "^1.2.0",
    "@types/n3": "^1.1.5"
  },
  "devDependencies": {
    "@comunica/runner": "^1.9.4",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "prepublishOnly": "yarn run build",
    "build:engine": "comunica-compile-config config/config-default.json > engine-default.js",
    "build:lib": "tsc",
    "build": "yarn run build:lib && yarn run build:engine"
  },
  "browser": {
    "./index.js": "./index-browser.js"
  }
}
